package parcialturnodtema110del10del23;

/**
 *
 * @author nacho
 */
public class Vehiculo {
    private String patente;
    private double cantHoras;
    private String marca;
    private String modelo;

    public Vehiculo(String patente, double cantHoras, String marca, String modelo) {
        this.patente = patente;
        this.cantHoras = cantHoras;
        this.marca = marca;
        this.modelo = modelo;
    }
    
    public boolean verificarMarca (String marcaX){
        return (this.marca.equals(marcaX));
    }
    
    public double retornarHoras (){
        return this.cantHoras;
    }
    
    @Override
    public String toString() {
        return "Vehiculo{" + "patente=" + patente + ", cantHoras=" + cantHoras + ", marca=" + marca + ", modelo=" + modelo + '}';
    }
    
    
    
}
